package demos

model AR1 {
  param RealVar psi ?: 1.01
  param Integer dim ?: 5
  
  random Matrix x ?: latentVector(dim)
  
  laws {
    x.getRealVar(0) ~ Normal(0, 1)
    for (int i : 1 ..< dim) {
      x.getRealVar(i) | psi, RealVar prev = x.getRealVar(i - 1) 
        ~ Normal(psi * prev, 1)
    }
  }
}
